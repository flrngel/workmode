{
  "manifest_version": 3,
  "name": "Work Mode Plus",
  "description": "Block ALL Time-Wasting Websites & Custom URL Blocking When You're On the Clock. Custom URL Block Extension. By flrngel. Originally: Shreyas Tallamraju.",
  "version": "3.0.2.2",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage"
  ], 
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Work Mode - Custom URL Blocking"
  },
  "commands": {
    "toggle_work_mode": {
      "suggested_key": {
        "default": "Alt+Shift+W"
      },
      "description": "Toggle Work Mode Plus on/off"
    }
  }
}
